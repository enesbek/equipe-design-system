  const hasTrailingSpace = query.endsWith(' ')\r\n    const searchRegex = new RegExp(\r\n      words\r\n      .map((word, index) => {\r\n        if (words.length === index + 1 && !hasTrailingSpace) {\r\n          // The last word - ok with the word being \"startswith\"-like\r\n          return `(?=.*\\\\b${escapeRegExp(word)})`\r\n        } else {\r\n          // Not the last word - expect the whole word exactly\r\n          return `(?=.*\\\\b${escapeRegExp(word)}\\\\b)`\r\n        }\r\n      })\r\n      .join('') + '.+',\r\n      'gi'\r\n    )\r\n    return searchRegex.test(domain)\r\n  } else {\r\n    // if the query has non-ASCII chars, treat as other languages\r\n    return words.some(word => domain.toLowerCase().indexOf(word) > -1)\r\n  }\r\n}\r\n"],"mappings":"AACA,OAAOA,GAAP,MAAgB,YAAhB;AAEA,gBAAe,CAACC,KAAD,EAAQC,IAAR,EAAcC,aAAa,GAAG,IAA9B,KAAuC;EACpD,IAAIC,MAAM,GAAGJ,GAAG,CAACE,IAAD,EAAO,OAAP,EAAgB,EAAhB,CAAhB;;EAEA,IAAIF,GAAG,CAACE,IAAD,EAAO,kBAAP,CAAP,EAAmC;IACjCE,MAAM,IAAK,IAAGF,IAAI,CAACG,WAAL,CAAiBC,IAAjB,CAAsBC,IAAtB,CAA2B,GAA3B,CAAgC,EAA9C;EACD;;EAED,IAAIJ,aAAJ,EAAmB;IACjBC,MAAM,IAAK,IAAGD,aAAc,EAA5B;EACD;;EAED,OAAOK,SAAS,CAACP,KAAD,EAAQG,MAAR,CAAhB;AACD,CAZD;;AAcA,MAAMI,SAAS,GAAG,CAACP,KAAD,EAAQG,MAAR,KAAmB;EACnC,MAAMK,YAAY,GAAGC,GAAG,IAAIA,GAAG,CAACC,OAAJ,CAAY,uBAAZ,EAAqC,MAArC,CAA5B,CADmC,CAGnC;;;EACA,MAAMC,cAAc,GAAG,IAAIC,MAAJ,CAAW,cAAX,CAAvB;EAEA,MAAMC,KAAK,GAAGb,KAAK,CAChBc,KADW,CACL,MADK,EAEXC,GAFW,CAEPN,GAAG,IAAIA,GAAG,CAACO,IAAJ,EAFA,EAGXC,MAHW,CAGJR,GAAG,IAAI,CAAC,CAACA,GAHL,CAAd;;EAKA,IAAI,CAACE,cAAc,CAACO,IAAf,CAAoBlB,KAApB,CAAL,EAAiC;IAC/B;IACA,MAAMmB,gBAAgB,GAAGnB,KAAK,CAACoB,QAAN,CAAe,GAAf,CAAzB;IACA,MAAMC,WAAW,GAAG,IAAIT,MAAJ,CAClBC,KAAK,CACJE,GADD,CACK,CAACO,IAAD,EAAOC,KAAP,KAAiB;MACpB,IAAIV,KAAK,CAACW,MAAN,KAAiBD,KAAK,GAAG,CAAzB,IAA8B,CAACJ,gBAAnC,EAAqD;QACnD;QACA,OAAQ,WAAUX,YAAY,CAACc,IAAD,CAAO,GAArC;MACD,CAHD,MAGO;QACL;QACA,OAAQ,WAAUd,YAAY,CAACc,IAAD,CAAO,MAArC;MACD;IACF,CATD,EAUChB,IAVD,CAUM,EAVN,IAUY,IAXM,EAYlB,IAZkB,CAApB;IAcA,